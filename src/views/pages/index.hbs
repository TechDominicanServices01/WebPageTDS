<section id="formulario" class="formularioContacto ">

    <div class="container">
        <div class="text-center pb-5">
            <h1 id="MidTittle">Envíanos un mensaje</h1>
            <p class="pt-4">Te responderemos en menos de 24 horas. </p>
        </div>

        <div>
            <form action="/" method="POST">
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label for="inputName">Nombre</label>
                        <input type="text" required  name="Nombre" class="form-control rounded" id="inputEmail4" placeholder="Nombre">
                    </div>
                    <div class="form-group col-md-6">
                        <label for="inputLastName">Apellido</label>
                        <input type="text" required name="Apellido" class="form-control rounded" id="inputPassword4" placeholder="Apellido">
                    </div>
                </div>

                 <div class="form-row">
                    <label for="inputLastName">Asunto</label>
                    <input type="text" required name="Asunto" class="form-control rounded" id="inputPassword4" placeholder="Asunto">
                </div>

                <div class="form-row">
                    <label for="inputLastName">Correo</label>
                    <input type="email" required name="Email" class="form-control rounded" id="inputPassword4" placeholder="Correo">
                </div>

                <div class="form-row pt-2">
                    <label for="inputAddress2">Mensaje</label>
                    <textarea required name="Mensaje" class="form-control rounded" id="" cols="30" rows="5"
                        placeholder="Mensjae"></textarea>
                </div>
                <div class="form-row mt-3 mb-5">
                    <button class="btn btn-dark btn-block rounded">Enviar</button>
                </div>

            </form>
        </div>

    </div>

</section>




